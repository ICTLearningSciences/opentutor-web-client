{
  "name": "opentutor-web-client",
  "description": "web ui for interacting with opentutor",
  "version": "1.0.0",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@reach/router": "^1.3.4",
    "@xapi/cmi5": "^0.4.0",
    "axios": "^0.21.1",
    "gatsby": "^2.25.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scroll": "^1.8.1",
    "reading-time": "^1.2.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-scroll": "^1.8.2",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby-cli": "^2.12.117",
    "gatsby-plugin-env-variables": "^2.0.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-material-ui": "^2.1.10",
    "gatsby-plugin-styled-components": "^3.3.14",
    "gatsby-plugin-manifest": "^2.5.2",
    "gatsby-plugin-typescript": "^2.5.0",
    "license-check-and-add": "^3.0.4",
    "npm-check-updates": "^9.2.4",
    "prettier": "^2.1.2",
    "typescript": "^4.0.5"
  },
  "keywords": [
    "gatsby"
  ],
  "resolutions": {
    "axios": "0.21.1"
  },
  "scripts": {
    "build": "npx gatsby build --prefix-paths",
    "develop": "npx gatsby develop",
    "format": "prettier --write '**/*.{js,jsx,json,ts,tsx}'",
    "start": "npm run develop",
    "serve": "gatsby serve --prefix-paths",
    "deps:update": "ncu -u",
    "test": "echo 'all tests currently cypress'",
    "test:all": "npx run test:format && npx run test:lint && npx run test:audit && npx run test:types && npx run test:cypress && npx test",
    "test:audit": "npm audit --audit-level=moderate --production",
    "test:cypress": "npx run cy:run --spec 'cypress/**/*.spec.js'",
    "test:format": "prettier --check '**/*.{js,jsx,json,ts,tsx}'",
    "test:lint": "eslint src --ext ts --ext tsx --ext js --ext jsx",
    "test:types": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ictlearningsciences/opentutor-web-client.git"
  },
  "bugs": {
    "url": "https://github.com/ictlearningsciences/opentutor-web-client/issues"
  }
}
