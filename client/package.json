    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-scroll": "^1.8.1",
    "axios": "^0.20.0",
    "babel-plugin-styled-components": "^1.11.1",
    "cors": "^2.8.5",
    "gatsby": "^2.24.76",
    "gatsby-plugin-manifest": "^2.4.34",
    "gatsby-plugin-material-ui": "^2.1.10",
    "gatsby-plugin-styled-components": "^3.3.14",
    "gatsby-plugin-typescript": "^2.4.21",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-scroll": "^1.8.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@hot-loader/react-dom": "^16.13.0",
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "acorn": "^8.0.4",
    "babel-eslint": "10.x",
    "babel-jest": "^26.5.2",
    "babel-preset-gatsby": "^0.5.14",
    "cypress": "^5.3.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "gatsby-cli": "^2.12.107",
    "gatsby-plugin-env-variables": "^2.0.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-root-import": "^2.0.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.5.3",
    "license-check-and-add": "^3.0.4",
    "npm-check-updates": "^9.0.4",
    "npm-force-resolutions": "0.0.3",
    "prettier": "^2.1.2",
    "react-refresh": "^0.8.3",
    "react-test-renderer": "^16.13.1",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3",
    "webpack": "5.x"
  },
  "resolutions": {
    "dot-prop": ">=5.1.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npx gatsby build --prefix-paths",
    "develop": "npx gatsby develop",
    "format": "prettier --write '**/*.{js,jsx,json,ts,tsx}'",
    "start": "npm run develop",
    "serve": "gatsby serve --prefix-paths",
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run",
    "cy:verify": "npx cypress verify",
    "deps:update": "ncu -u",
    "test": "jest",
    "test:all": "npx run test:format && npx run test:lint && npx run test:audit && npx run test:types && npx run test:cypress && npx test",
    "test:audit": "npm audit --audit-level=moderate --production",
    "test:cypress": "npx run cy:run --spec  'cypress/**/*.spec.js'",
    "test:format": "prettier --check '**/*.{js,jsx,json,ts,tsx}'",
    "test:lint": "eslint src --ext ts --ext tsx --ext js --ext jsx",
    "test:types": "tsc --noEmit",
    "test:watch": "jest --watch",
    "license:fix": "license-check-and-add remove -f license-config.json && license-check-and-add add -f license-config.json",
    "test:license": "license-check-and-add check -f license-config.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ictlearningsciences/opentutor-web-client.git"
  },
  "bugs": {
    "url": "https://github.com/ictlearningsciences/opentutor-web-client/issues"
  }
}
