{
  "name": "opentutor-web-client-node",
  "version": "1.0.19",
  "private": true,
  "scripts": {
    "format": "prettier --write '**/*.{js,jsx,json,ts,tsx}'",
    "start": "node app",
    "start-dev": "node app dev",
    "version": "git add -A",
    "postversion": "git push",
    "serve": "gatsby serve",
    "test": "echo 'no tests defined yet'",
    "test:all": "npm run test:format && npm run test:lint && npm run test:audit && npm run test:types && npm test",
    "test:audit": "npm audit --audit-level=moderate",
    "test:format": "prettier --check '**/*.{js,jsx,json,ts,tsx}'",
    "test:lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
    "test:types": "tsc --noEmit",
    "license:fix": "license-check-and-add remove -f license-config.json && license-check-and-add add -f license-config.json",
    "test:license": "license-check-and-add check -f license-config.json"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "~4.16.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/eslint-plugin-tslint": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1"
  },
  "main": "app.js",
  "author": "",
  "license": "ISC"
}
